
public class MethodParams {
	public static void main(String[] args)
	{
		//class instance
		MethodParams mp = new MethodParams();
		
		//call method
		double d = mp.convertIntToDouble(44);
		
		System.out.println(d);
	}
	//create method
	public double convertIntToDouble(int num)
	{
		return (double)num;
	}
}
